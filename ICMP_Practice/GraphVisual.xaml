<Window x:Class="ICMP_Practice.GraphVisual"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ICMP_Practice"
        mc:Ignorable="d"
        Title="GraphVisual" Height="600" Width="1000">
    <Grid x:Name="MainGrid">
        <Image x:Name="sheet" Stretch="Uniform"></Image>
        <Button x:Name="SavePictureButton" Content="Сохранить в png" 
                HorizontalAlignment="Left" VerticalAlignment="Top"
                Height="40" Width="120" Margin="10" Click="SavePictureButton_Click">
            
        </Button>
    </Grid>
</Window>
